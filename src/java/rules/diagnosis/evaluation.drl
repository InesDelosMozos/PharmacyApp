import diagnosis.Patient

rule "digoxin1"
    //salience 15
    dialect "java"
    when
        p1: Patient  (disease.equals("heart failure"), age != Age.CHILD,
    !medicines.contains("diuretics"), !comorbidity.contains("allergic to glycosides"))

then
    p1.setTreatments("digoxin");
    System.out.println("Digoxin is the best treatment for the patient");
end
